
\section*{Неравенства.}
\setcounter{z}{0}


Решите неравенства.

\zad  $  243^x-3^{5x-2}\ge7.$

\zad  $ 4\cdot 4^{x}-33\cdot2^{x}+8\le0.$

\zad  $ 4^{x^2-x}-10\cdot2^{x^2}+2^{2x+4}\ge0.$

\zad  $ \dfrac{6}{4^{\sqrt{x}}-3\cdot2^{\sqrt{x}}+2}<1.$


\zad $ 2^{\frac{x}{x+1}}-2^{\frac{5x+3}{x+1}}+8\le 2^{\frac{2x}{x+1}}.$


\zad    $4^x+4^{-x}\geq1,7\left(2^x+2^{-x} \right) .$

\zad    $\log_{\sqrt{x+2}-\sqrt{x-1}}(x^2+x)\ge0.$

\zad $ 2^{\log^2_2x}+x^{\log_2x}\le256.$

\zad  $\log_7(x+2)-3\log_{x+2}7+2>0.$
 
 \zad $ \log_{x^2}{(x-1)^2}\le1.$
            
\zad $\log_{\frac1x}(2,5x-1)\ge-2.$
            


\zad  $\dfrac{1}{\log_{27x}x}\ge\log_3\dfrac{x}3.$
            

\zad $ \log_{x^2+1}(x-3)^2\cdot \log_{x^2+1}\left(\dfrac{(x-3)^2}{(x^2+1)^3}\right)\le-2.$

\zad    $\dfrac{2^{\log_{\frac{x}{2}}3}-3}{2-\log_{\frac{x}{128}}{\frac{x}{64}}}\ge0.$

\zad $ (4^{x^2-x-6}-1)\cdot\log_{0,25}(4^{x^2+2x+2}-3)\le0.$

\zad $\log_{9x}{3x}+\log_{3x^2}{9x^2}\le2,5.$


\zad  $\log_{x+1}(4x^2-4x+1)\cdot\log_{1-2x}(6x+6)\ge2.$

\zad    $\left( x+\dfrac{4}{x}\right)\cdot\log^2_{6-x}(x^2-8x+16)\ge5\cdot\log^2_{6-x}(x^2-8x+16).$

\zad    $\log_{|x+3|}(2+4x+x^2)\le2.$

\zad    $\log_{x^2}\left(\dfrac{4x-5}{x-2} \right)^2\le1.$

 
\zad   $4^x\cdot\log_{x^2}(x+2)^6\le14^x\cdot\log_{x^2}(x+2)^{11}+7^{2x+1}\cdot\log_{x^2}(x+2)^{5}.$

\zad   $\log_{x^2-2}(x^4-4)\le 2+\dfrac{1}{\log_3(x^2-2)}.$


\zad    $\log_{0,5}\left(5^{1+\lg x}-0,5^{1+\lg x} \right)\ge-1+\lg x.$

\zad    $\left| \log_{x+1}\sqrt{(x-2)^4}+2 \right|\ge\log_{\frac{1}{x+1}}\sqrt{(x-2)^6}-3.$


\zad    $\left| \log_{x+1}2+\log_2\dfrac{x+1}4 \right|+|\log_2(4x+4)+\log_{x+1}2 |<5.$


\zad    $\dfrac{9}{\log_{1,9}x\log_{2,1}(x-10)^2}\ge \dfrac{(x-1)^{\log_3(x-1)}}{9\log_{1,9}x\log_{2,1}(x-10)^2}.$

\zad    $\dfrac{3\lg x-\log_x10x^2}{2\lg x+\log_x1000x^5}\le\dfrac{2\lg^2x-\lg\dfrac{x^3}{10}}{3\lg\dfrac{x}{10}\lg10x+7\lg10x}.$

\zad    $\dfrac{\log_4(x^4-4x^3+4x^2)+\log_{0,25}(6x^2-12x-9)}{x^2-2x-8}\ge0.$

\zad    $x\ge \log_2(101\cdot10^x-10^{2+2x})-\log_5(101\cdot2^x-5^{2+x}\cdot2^{2+2x}).$

\zad    $\sqrt{2x^2+3x-35}+\sqrt{2x^2+x-36}\le|2x+1|.$
